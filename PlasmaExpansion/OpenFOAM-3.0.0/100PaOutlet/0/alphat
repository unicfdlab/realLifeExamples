FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    INLET
    {
        type            calculated;
        value           uniform 0;
    }
    OUTLET
    {
        type            calculated;
        value           uniform 0;
    }
    DEPOSITOR
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    WALL
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    NWALL
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    axi_symm-r
    {
        type            wedge;
    }
    axi_symm-f
    {
        type            wedge;
    }
    defaultFaces
    {
        type            empty;
    }
}


